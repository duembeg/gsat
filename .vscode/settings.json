{
    "spellright.language": [
        "en_US"
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "scminput"
    ],
    "spellright.parserByClass": {
        "scminput": {
            "parser": "plain"
        }
    },
    "cSpell.words": [
        "EVENTIF",
        "INET",
        "MPOS",
        "REMOTEIF",
        "REUSEADDR",
        "RXDATA",
        "SERIALIF",
        "TXDATA",
        "datefmt",
        "gsat",
        "machif"
    ],
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.inheritEnv": false,

    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/kivy-rc/bin/**": true,
        "**/kivy-rc/.buildozer/**": true
      }
}
